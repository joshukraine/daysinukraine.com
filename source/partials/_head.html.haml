= tag "meta", charset: "utf-8"
= tag "meta", "http-equiv" => 'X-UA-Compatible', content: 'IE=edge;chrome=1'
= tag "meta", name: "viewport", content: "width=device-width, initial-scale=1"

= favicon_tag "favicon.png"
= tag "link", rel: "apple-touch-icon", href: "images/apple-touch-icon.png"

= stylesheet_link_tag "site"
= javascript_include_tag :bundle, async: true

%title
  #{current_page.data.title + ' - ' unless current_page.data.title.nil?} Nathan & Katelin Day
= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed"
